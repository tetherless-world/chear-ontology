cogat:CAO_00118 a owl:Class ;
    rdfs:label "Amodal representation" ;
    dc:contributor "Corey White" ;
    dc:date "2011-08-11 19:18:12"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00383 ;
    skos:definition "The way the brain codes multiple inputs such as words and pictures to integrate and create a larger conceptual idea; independent of a particular modality" .

cogat:CAO_00119 a owl:Class ;
