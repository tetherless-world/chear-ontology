    dc:date "2011-08-14 23:10:33"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00115 ],
        cogat:CAO_00001 ;
    skos:definition "The act of understanding, being aware of, being sensitive to, and or experiencing the feelings, thoughts, and experience of another. " .

cogat:CAO_00574 a owl:Class ;
    rdfs:label "Fear" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-04-01 17:45:41"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00816 ;
    skos:altLabel "acute threat" ;
    skos:definition "A state of high negative emotional arousal triggered by an impending threat (real or imaginary) and generally associated with the flight or fight response. Fear is the activation of the brain’s defensive motivational system to promote behaviors that protect the organism from perceived danger. Normal fear involves a pattern of adaptive responses to conditioned or unconditioned threat stimuli (exteroceptive or interoceptive). Fear can involve internal representations and cognitive processing, and can be modulated by a variety of factors." .

cogat:CAO_00598 a owl:Class ;
    rdfs:label "Sentence comprehension" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-04-12 18:54:01"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00360 ],
        cogat:CAO_00361 ;
    skos:definition "takes place whenever a reader or listener processes a language utterance, either in isolation or in the context of a conversation or a text." .

cogat:CAO_00603 a owl:Class ;
    rdfs:label "Consolidation" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-09-18 17:06:39"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00373 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00617 ],
        cogat:CAO_00001 ;
    skos:definition "the process by which a representation becomes stabilized" .

cogat:CAO_00610 a owl:Class ;
    rdfs:label "Memory consolidation" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 0:34:05"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00603 ;
    skos:definition "a category of processes that stabilize a memory trace after the initial acquisition." .

cogat:CAO_00612 a owl:Class ;
    rdfs:label "Memory storage" ;
    dc:contributor "Tal Yarkoni" ;
    dc:date "2010-04-27 14:32:05"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00468 ],
        cogat:CAO_00001 ;
    skos:definition "The representation of information in the brain in a form that enables potential retrieval at a later time." .

cogat:CAO_00617 a owl:Class ;
    rdfs:label "Reconsolidation" ;
    dc:contributor "Brenda Gregory" ;
    dc:date "2011-04-29 0:26:42"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00610 ;
    skos:definition "the process of previously consolidated memories being recalled and actively consolidated, it is a distinct process that serves to maintain, strengthen and modify memories that are already stored in the long-term memory." .

cogat:CAO_00623 a owl:Class ;
