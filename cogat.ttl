cogat:CAO_00109 a owl:Class ;
    rdfs:label "Activation" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 15:41:03"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the relative engagement of a particular mental representation compared to other representations" .

cogat:CAO_00110 a owl:Class ;
    rdfs:label "Activation level" ;
    dc:contributor "Corey White" ;
    dc:date "2011-08-11 15:26:14"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
