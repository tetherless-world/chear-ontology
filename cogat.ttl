    rdfs:label "Abductive reasoning" ;
    dc:contributor "Corey White" ;
    dc:date "2011-08-10 20:41:26"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00459 ;
    skos:definition "The process of adopting an explanatory hypothesis; inferring the cause A as a possible explanation for the consequence B" .

cogat:CAO_00102 a owl:Class ;
