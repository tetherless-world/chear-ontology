    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 15:36:29"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00359 ;
    skos:definition "Knowledge that is general and not tied to a specific instance." .

