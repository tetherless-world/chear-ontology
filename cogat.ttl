            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00160 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00511 ],
        cogat:CAO_00160 ;
    skos:definition "the cognitive ability to discriminate items on the basis of contrasts in sonorance, manner, place, or voicing in auditory stimuli. " .

cogat:CAO_00107 a owl:Class ;
    rdfs:label "Acoustic processing" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 15:37:24"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
