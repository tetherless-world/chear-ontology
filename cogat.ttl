cogat:CAO_00929 a owl:Class ;
    rdfs:label "Wechsler memory scale fourth edition" ;
    dc:contributor "Nolan Nichols" ;
    dc:date "2010-03-08 8:11:23"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
