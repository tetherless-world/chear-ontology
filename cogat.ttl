cogat:CAO_00555 a owl:Class ;
    rdfs:label "Word order" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-03-09 18:52:14"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "is the sequence of the syntactic constituents of a language." .

cogat:CAO_00556 a owl:Class ;
    rdfs:label "Word pronunciation" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-04-12 19:45:19"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00554 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00360 ],
        cogat:CAO_00001 ;
    skos:definition "is the act or result of producing the sounds of speech, including articulation, stress, and intonation, often with reference to some standard of correctness or acceptability." .

cogat:CAO_00558 a owl:Class ;
    rdfs:label "Word repetition" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2014-04-10 12:41:45"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "overt repetition" ;
    skos:definition "Overt pronounciation of auditorily presented words" .

cogat:CAO_00560 a owl:Class ;
    rdfs:label "Action initiation" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 15:29:57"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the facilitation or initiation of an act" .

cogat:CAO_00561 a owl:Class ;
    rdfs:label "Attention shift" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 15:57:29"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The change that occurs when information that is currently active in the mind is replaced by other information. The information content is typically sensory in nature." .

cogat:CAO_00562 a owl:Class ;
    rdfs:label "Attention span" ;
    dc:contributor "Agatha Lenartowicz" ;
    dc:date "2009-07-10 21:06:33"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00141 ],
        cogat:CAO_00001 ;
    skos:definition "Amount of time or space that an individual can dedicate to particular task or content without becoming distracted." .

cogat:CAO_00563 a owl:Class ;
    rdfs:label "Attentional focusing" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 15:58:45"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00141 ],
        cogat:CAO_00001 ;
    skos:definition "The ability to focus attention on cues in the environment that are relevant to the task in hand; can also include suppression of distracting stimuli" .

cogat:CAO_00564 a owl:Class ;
    rdfs:label "Attention shifting" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 15:57:46"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The process by which information that is currently relevant in the mind is replaced by other information. This information is typically sensory in nature but may also be semantic." .

cogat:CAO_00568 a owl:Class ;
    rdfs:label "Framing" ;
    dc:contributor "Naomi Kenner" ;
    dc:date "2009-07-10 21:06:33"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "Framing is a method of biasing opinions- a framing effect occurs when the description of information, such as a speaker presenting an issue, has an emphasis on a subset of potentially relevant considerations and causes individuals to focus on these considerations when constructing their opinions." .

cogat:CAO_00569 a owl:Class ;
    rdfs:label "Risk seeking" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-11-09 0:07:29"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00832 ],
        cogat:CAO_00001 ;
    skos:definition "The willingness of an individual to choose an option with a less-than-certain probability of reward over an option with a certain reward of equal or higher expected value.  BEHAVIORAL PHENOMENA" .

cogat:CAO_00570 a owl:Class ;
    rdfs:label "Emotion perception" ;
    dc:contributor "Tyler Davis" ;
    dc:date "2011-08-14 22:59:25"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The process involving understanding feelings with different valences of oneself or of others" .

cogat:CAO_00572 a owl:Class ;
    rdfs:label "Emotional bonding" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 17:55:11"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "A process of bringing people together based on mutually shared emotions." .

cogat:CAO_00575 a owl:Class ;
    rdfs:label "Frustration" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 19:00:18"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a deep chronic sense or state of insecurity and dissatisfaction arising from unresolved problems or unfulfilled needs." .

cogat:CAO_00576 a owl:Class ;
    rdfs:label "Grief" ;
    dc:contributor "Brenda Gregory" ;
    dc:date "2011-05-11 23:20:10"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "deep and poignant distress caused by or as if by bereavement." .

cogat:CAO_00577 a owl:Class ;
    rdfs:label "Happiness" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 19:51:32"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a state of mind or feeling characterized by contentment, love, satisfaction, pleasure, or joy." .

cogat:CAO_00578 a owl:Class ;
    rdfs:label "Discourse" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 16:49:41"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the capacity of orderly thought or procedure, verbal interchange of ideas; formal and orderly and usually extended expression of thought on a subject, connected speech or writing, linguistic unit larger than a sentence." .

cogat:CAO_00579 a owl:Class ;
    rdfs:label "Discourse comprehension" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 16:49:51"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00361 ;
    skos:definition "Discourse comprehension is the act of interpreting a written or spoken message by integrating the incoming information into the memory or knowledge structures of the interpreter." .

cogat:CAO_00580 a owl:Class ;
    rdfs:label "Discourse knowledge" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 16:50:22"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00359 ;
    skos:definition "knowledge regarding discourse" .

cogat:CAO_00581 a owl:Class ;
    rdfs:label "Discourse planning" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-04-07 18:16:27"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00433 ;
    skos:definition "the formulation of structures, patterns, mental representations, and processes that constitute the written and spoken unit of connected speech or writing longer than a sentence. " .

cogat:CAO_00582 a owl:Class ;
    rdfs:label "Discourse processing" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-04-07 18:30:15"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "is the cognitive process that investigates the structures, patterns, mental representations, and processes that underlie the written and spoken unit of connected speech or writing longer than a sentence.  " .

cogat:CAO_00583 a owl:Class ;
    rdfs:label "Discourse production" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-04-07 18:18:02"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the formation of a spoken unit of connected speech or writing longer than a sentence." .

cogat:CAO_00584 a owl:Class ;
    rdfs:label "Grammatical encoding" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-04-11 21:23:28"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00605 ;
    skos:definition "the selection of semantically appropriate lexical items and the generation of a syntactic frame or surface form." .

cogat:CAO_00585 a owl:Class ;
    rdfs:label "Grapheme" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 19:01:59"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a unit (as a letter or digraph) of a writing system; the set of units of a writing system (as letters and letter combinations) that represent a phoneme." .

cogat:CAO_00586 a owl:Class ;
    rdfs:label "Graphemic buffer" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 19:02:09"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a component dedicated to the temporary storage of abstract orthographic representations prior to their format-specific expression in spelling and-or reading." .

cogat:CAO_00587 a owl:Class ;
    rdfs:label "Illocutionary force" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 21:30:10"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a speakerand#39;s intention in delivering an utterance." .

cogat:CAO_00588 a owl:Class ;
    rdfs:label "Language acquisition" ;
    dc:contributor "Brenda Gregory" ;
    dc:date "2011-04-19 20:50:53"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the process by which humans acquire the capacity to perceive, produce and use words to understand and communicate." .

cogat:CAO_00590 a owl:Class ;
    rdfs:label "Lexical access" ;
    dc:contributor "Brenda Gregory" ;
    dc:date "2011-04-19 19:19:05"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the process by which contact is made with the lexicon on the basis of an initial acoustic-phonetic or phonological representation of some portion of the speech input, the result of lexical success is a cohort of potential word candidates which are compatible with this initial analysis." .

cogat:CAO_00591 a owl:Class ;
    rdfs:label "Lexical ambiguity" ;
    dc:contributor "Brenda Gregory" ;
    dc:date "2011-05-16 18:10:04"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the presence of two or more possible meanings within a single word." .

cogat:CAO_00593 a owl:Class ;
    rdfs:label "Morphological processing" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-04-12 18:56:07"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "is how the brain registers the patterns of word formation in a particular language, including inflection, derivation, and composition. " .

cogat:CAO_00594 a owl:Class ;
    rdfs:label "Morphology" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 0:37:07"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the structure and content of word forms" .

cogat:CAO_00595 a owl:Class ;
    rdfs:label "Orthography" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 0:53:27"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the representation of the sounds of a language by written or printed symbols; a part of language study that deals with letters and spelling." .

cogat:CAO_00596 a owl:Class ;
    rdfs:label "Phonetics" ;
    dc:contributor "Brenda Gregory" ;
    dc:date "2011-04-29 0:22:02"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the system of speech sounds of a language or group of languages; the study and systematic classification of the sounds made in spoken utterance." .

cogat:CAO_00597 a owl:Class ;
    rdfs:label "Semantic processing" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 1:32:48"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00834 ],
        cogat:CAO_00001 ;
    skos:definition "refers to the cognitive processing of the meanings of a stimulus" .

cogat:CAO_00600 a owl:Class ;
    rdfs:label "Sentence production" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 1:35:22"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The creation and-or utterance of a sentence" .

cogat:CAO_00601 a owl:Class ;
    rdfs:label "Active recall" ;
    dc:contributor "Brenda Gregory" ;
    dc:date "2011-05-16 22:03:14"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a principle of efficient learning, which claims the need to actively stimulate memory during the learning process." .

cogat:CAO_00602 a owl:Class ;
    rdfs:label "Active retrieval" ;
    dc:contributor "Corey White" ;
    dc:date "2011-08-11 15:29:15"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00285 ],
        cogat:CAO_00001 ;
    skos:definition "Effortful (volitional) attempt to consciously recollect a memory; often required when retrieval cannot be automatically driven by stimuli." .

cogat:CAO_00604 a owl:Class ;
    rdfs:label "Elaborative rehearsal" ;
    dc:contributor "Brenda Gregory" ;
    dc:date "2011-05-03 20:53:58"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a type of rehearsal proposed by Craik and Lockhart (1972) in their Levels of Processing model of memory. Elaborative rehearsal involves deep semantic processing of a to-be-remembered item resulting in the production of durable memories." .

cogat:CAO_00606 a owl:Class ;
    rdfs:label "Episodic buffer" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 17:58:37"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00559 ],
        cogat:CAO_00001 ;
    skos:definition "A theoretical construct that is part of Alan Baddeleyand#39;s working memory model and the object of which is to integrate information across sensory domains and communicate with long term memory in the service of working memory." .

cogat:CAO_00607 a owl:Class ;
    rdfs:label "Familiarity" ;
    dc:contributor "Tyler Davis" ;
    dc:date "2011-08-14 23:54:36"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00248 ],
        cogat:CAO_00001 ;
    skos:definition "A quality of memory that is associated with a feeling of knowing that an event has previously occurred, but often not including enough contextual or episodic details sufficient for claiming actual remembering of the event." .

cogat:CAO_00609 a owl:Class ;
    rdfs:label "Memory acquisition" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-04-12 18:13:11"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "is the process of storage and retrieval of new information in memory." .

cogat:CAO_00611 a owl:Class ;
    rdfs:label "Memory decay" ;
    dc:contributor "Brenda Gregory" ;
    dc:date "2011-05-17 16:57:43"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the loss of memory over time." .

cogat:CAO_00613 a owl:Class ;
    rdfs:label "Memory trace" ;
    dc:contributor "Tal Yarkoni" ;
    dc:date "2010-04-27 14:39:30"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00633 ],
        cogat:CAO_00001 ;
    skos:definition "A residual, and often decayed, neural representation of previous knowledge or experience." .

cogat:CAO_00614 a owl:Class ;
    rdfs:label "Metamemory" ;
    dc:contributor "Michael Sharman" ;
    dc:date "2012-02-23 16:51:52"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00385 ;
    skos:definition "subjective awareness of and knowledge about memory, and strategies for its effective access and application; includes: awareness of memory contents, different and appropriate memory strategies for particular tasks, and how to use a given memory strategy most effectively." .

cogat:CAO_00618 a owl:Class ;
    rdfs:label "Acuity" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 15:43:14"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "accuracy of perception." .

cogat:CAO_00619 a owl:Class ;
    rdfs:label "Echolocation" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 17:52:35"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the general method of locating objects by determining the time for an echo to return and the direction from which it returns, used by some animals" .

cogat:CAO_00620 a owl:Class ;
    rdfs:label "Edge detection" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 17:53:12"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00621 ;
    skos:definition "a fundamental tool in image processing and computer vision, particularly in the areas of feature detection and feature extraction, which aim at identifying points in a digital image at which the image brightness changes sharply or more formally has discontinuities." .

cogat:CAO_00622 a owl:Class ;
    rdfs:label "Figure ground reversal" ;
    dc:contributor "Tyler Davis" ;
    dc:date "2011-08-15 0:11:06"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "Occurs in certain visual illusions (e.g., Vases and Faces or and#34;Rubinand#39;s Vaseand#34;) in which there are multiple valid figure-ground segmentations that can be selected depending on an observerand#39;s interpretation." .

cogat:CAO_00625 a owl:Class ;
    rdfs:label "Memory retrieval" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 0:34:48"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00616 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00615 ],
        cogat:CAO_00469 ;
    skos:definition "the process of accessing a stored memory" .

cogat:CAO_00626 a owl:Class ;
    rdfs:label "Active maintenance" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2010-07-14 18:39:19"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00559 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00418 ],
        cogat:CAO_00882 ;
    skos:definition "The maintenance of information in working memory through active (volitional) rather than passive means" .

cogat:CAO_00627 a owl:Class ;
    rdfs:label "Articulatory loop" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 15:55:18"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00559 ],
        cogat:CAO_00633 ;
    skos:altLabel "phonological loop" ;
    skos:definition "one of the subsystems postulated in Alan Baddeley’s multicomponent model of WORKING MEMORY, specialized for the temporary storage of verbal information.  It consists of a phonological store and an articulatory rehearsal process. The phonological store can hold speech-based information, subject to a rapid decay. The articulatory rehearsal process can refresh the decaying representation by reading it off and feeding it back to the store. It also serves to convert visually presented information (such as written words) into phonological codes and register them into the phonological store. " .

cogat:CAO_00628 a owl:Class ;
    rdfs:label "Central executive" ;
    dc:contributor "Michael Sharman" ;
    dc:date "2012-05-03 12:46:50"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00636 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00559 ],
        cogat:CAO_00001 ;
    skos:definition "Theoretical and#39;top-downand#39; cognitive system responsible for the management of executive functions including planning, working memory, attention, problem solving, reasoning, inhibition, multi-tasking, and the initiation and monitoring of actions." .

cogat:CAO_00630 a owl:Class ;
    rdfs:label "Manipulation" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 0:30:27"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "performance of operations upon information held in working memory" .

cogat:CAO_00631 a owl:Class ;
    rdfs:label "Phonological loop" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 0:58:07"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00167 ],
        cogat:CAO_00633 ;
    skos:definition "deals with sound or phonological information and consists of two parts: a short-term phonological store with auditory  memory traces that are subject to rapid decay and an articulatory rehearsal component that can revive the memory traces." .

cogat:CAO_00634 a owl:Class ;
    rdfs:label "Working memory retrieval" ;
    dc:contributor "Agatha Lenartowicz" ;
    dc:date "2009-07-10 21:06:33"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00559 ],
        cogat:CAO_00001 ;
    skos:definition "The process of accessing information that is maintained in working memory; the sub-process by which the contents of working memory are accessed." .

cogat:CAO_00635 a owl:Class ;
    rdfs:label "Working memory storage" ;
    dc:contributor "Agatha Lenartowicz" ;
    dc:date "2009-07-10 21:06:33"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00559 ],
        cogat:CAO_00001 ;
    skos:definition "The maintenance of information of working memory; a sub-component of working memory that allows for contents of working memory to be retained." .

cogat:CAO_00637 a owl:Class ;
    rdfs:label "Echoic memory" ;
    dc:contributor "Brenda Gregory" ;
    dc:date "2011-05-12 22:21:16"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00159 ;
    skos:definition "refers to the phenomenon in which there is a brief mental echo that continues to sound after an auditory stimulus has been heard." .

cogat:CAO_00638 a owl:Class ;
    rdfs:label "Reward processing" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-11-30 3:43:35"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00740 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00677 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00738 ],
        cogat:CAO_00823 ;
    skos:altLabel "reinforcement",
        "reward" ;
    skos:definition "A positive return for performance of a specific behavior." .

cogat:CAO_00641 a owl:Class ;
    rdfs:label "Behavioral inhibition (cognitive)" ;
    dc:contributor "System Administrator" ;
    dc:date "2010-02-17 17:27:37"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00682 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00636 ],
        cogat:CAO_00001 ;
    skos:altLabel "behavioral inhibition" ;
    skos:definition "Often used as a synonym of and#34;response inhibitionand#34; to describe the inhibition of actions." .

cogat:CAO_00642 a owl:Class ;
    rdfs:label "Behavioral inhibition (temperament)" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2010-02-17 17:27:37"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "behavioral inhibition" ;
    skos:definition "A temperamental characteristic described by shyness and social anxiety." .

cogat:CAO_00643 a owl:Class ;
    rdfs:label "Central coherence" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-07-11 23:24:03"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a personand#39;s ability to understand things in context, frequently described as a deficit in literature on autism" .

cogat:CAO_00646 a owl:Class ;
    rdfs:label "Task set" ;
    dc:contributor "Mei-Yen Chen" ;
    dc:date "2011-08-12 20:09:15"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The set of rules and-or stimulus-response mappings that define how a task should be performed." .

cogat:CAO_00648 a owl:Class ;
    rdfs:label "Semantic information" ;
    dc:contributor "Akram Bakkour" ;
    dc:date "2011-08-12 21:10:20"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "semantic knowledge" ;
    skos:definition "information that is not tied to any specific object, event, domain, or application. It includes general factual information about the world (as in an encyclopedia) and oneself." .

cogat:CAO_00649 a owl:Class ;
    rdfs:label "Paraphasia" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-04-11 21:24:31"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "generally refers to errors in naming. Collectively, this term is applied to any unintended error of word or sound choice. " .

cogat:CAO_00650 a owl:Class ;
    rdfs:label "Phonemic paraphasia" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-04-11 21:24:12"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "is the production of unintended sounds or syllables in the utterance of partially recognizable word, e.g., ‘paker’ for ‘paper.’ " .

cogat:CAO_00651 a owl:Class ;
    rdfs:label "Neologism" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-04-11 20:28:35"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "is the production of nonsense word or words, usually without recognition of errors, e.g., ‘table’ becomes ‘tilto.’  " .

cogat:CAO_00652 a owl:Class ;
    rdfs:label "Metacomprehension" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-05-16 20:35:40"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "refers to the learnersand#39; ability to monitor the degree to which they understand information being communicated to them, to recognize failures to comprehend, and to employ repair strategies when failures are identified. " .

cogat:CAO_00653 a owl:Class ;
    rdfs:label "Conceptual metaphor" ;
    dc:contributor "Natalie Picchetti" ;
    dc:date "2011-04-01 19:33:23"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "In cognitive linguistics, metaphor is defined as understanding one conceptual domain in terms of another conceptual domain; for example, using one personand#39;s life experience to understand a different personand#39;s experience. A conceptual domain can be any coherent organization of experience.  " .

cogat:CAO_00655 a owl:Class ;
    rdfs:label "Delusion" ;
    dc:contributor "Michael Sharman" ;
    dc:date "2012-05-04 10:30:07"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "A belief held despite appreciable external evidence to the contrary." .

cogat:CAO_00656 a owl:Class ;
    rdfs:label "Deliberation" ;
    dc:contributor "Michael Sharman" ;
    dc:date "2012-05-03 16:48:27"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "consideration" ;
    skos:definition "the process of judging the merits of multiple options, which may then be followed by choice." .

cogat:CAO_00657 a owl:Class ;
    rdfs:label "Thought" ;
    dc:contributor "Michael Sharman" ;
    dc:date "2012-05-03 14:24:18"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "Subjectively salient cognitive processing, at least part of which may be reportable." .

cogat:CAO_00658 a owl:Class ;
    rdfs:label "Multisensory integration" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 0:45:26"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The process by which unisensory signals are combined to form a new product. It is operationally defined as a multisensory response (neural or behavioral) that is significantly different from the responses evoked by the modality-specific component stimuli" .

cogat:CAO_00659 a owl:Class ;
    rdfs:label "Crossmodal" ;
    dc:contributor "Michael Sharman" ;
    dc:date "2014-01-02 13:52:05"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "cross-modal" ;
    skos:definition "A complex of two or more modality-specific [unimodal] stimuli" .

cogat:CAO_00660 a owl:Class ;
    rdfs:label "Unisensory" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 1:57:06"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "Describes any sensory process associated with a single sensory modality" .

cogat:CAO_00661 a owl:Class ;
    rdfs:label "Multisensory" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 0:43:06"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a process associated with multiple sensory modalities" .

cogat:CAO_00662 a owl:Class ;
    rdfs:label "Synchrony perception" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 1:49:30"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00418 ;
    skos:definition "the process of perceiving whether or not the crossmodal cues (e.g., audio and visual) to an event (e.g., audiovisual speech) are in temporal synchrony with each other." .

cogat:CAO_00663 a owl:Class ;
    rdfs:label "Tactile working memory" ;
    dc:contributor "Mei-Yen Chen" ;
    dc:date "2011-08-12 20:08:59"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00559 ;
    skos:definition "working memory for tactile information" .

cogat:CAO_00664 a owl:Class ;
    rdfs:label "Interoceptive representation" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2011-08-24 14:09:26"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00681 ],
        cogat:CAO_00654 ;
    skos:definition "A representation of the internal state of the body" .

cogat:CAO_00666 a owl:Class ;
    rdfs:label "Sense of body ownership" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-26 18:05:05"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00665 ;
    skos:definition "The feeling that something is part of oneand#39;s own body." .

cogat:CAO_00667 a owl:Class ;
    rdfs:label "Pain sensitization" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 0:54:08"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00639 ],
        cogat:CAO_00001 ;
    skos:definition "A painful stimulus is perceived more painful over the time course of stimulation." .

cogat:CAO_00668 a owl:Class ;
    rdfs:label "Pain habituation" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 0:53:54"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00639 ],
        cogat:CAO_00001 ;
    skos:altLabel "pain adaptation" ;
    skos:definition "A painful stimulus is perceived less painful over the time course of stimulation." .

cogat:CAO_00670 a owl:Class ;
    rdfs:label "Expectancy" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-04-01 17:54:13"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00127 ],
        cogat:CAO_00001 ;
    skos:altLabel "expectation",
        "reward prediction error" ;
    skos:definition "A belief about something in the future. Sometimes requires explicit, conscious awareness, distinct from unconscious, conditioning-based learning." .

cogat:CAO_00674 a owl:Class ;
    rdfs:label "Curiosity" ;
    dc:contributor "System Administrator" ;
    dc:date "2012-04-20 23:17:21"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "inquisitiveness" ;
    skos:definition "wanting knowledge or understanding, often of a novel or unfamiliar entity  " .

cogat:CAO_00678 a owl:Class ;
    rdfs:label "Self talk" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-09-21 22:51:39"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "overt verbalisation of thoughts for the purpose of self motivation, problem solving, reasoning, learning or stress management; a behavior commonly witnessed in sports-people during moments of performance pressure " .

cogat:CAO_00679 a owl:Class ;
    rdfs:label "Worldview" ;
    dc:contributor "Michael Sharman" ;
    dc:date "2012-02-24 10:36:52"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "perspective" ;
    skos:definition "overarching subjective account of environmental (ie. that in which an agent is situated) characteristics and functions, applicable across local and global spatiotemporal scales; a framework of ego-centric ideas and beliefs that serves to guide decision making and action, and to generate, sustain, and apply knowledge. " .

cogat:CAO_00685 a owl:Class ;
    rdfs:label "Color constancy" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-26 19:28:32"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the ability to assign stable colors despite variation in light, i.e. identifying that a given box is orange irrespective of the spectral composition or intensity of the light illuminating it" .

cogat:CAO_00686 a owl:Class ;
    rdfs:label "Feature search" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-26 20:17:48"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00549 ;
    skos:definition "search for a target according to a single feature (e.g. color)" .

cogat:CAO_00688 a owl:Class ;
    rdfs:label "Change blindness" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-26 21:03:03"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00680 ;
    skos:definition "the inability to detect changes to an object or a scene even if large, repeatedly made, or anticipated; it can even occur when the subject is fixated on the item being changed." .

cogat:CAO_00689 a owl:Class ;
    rdfs:label "Motion aftereffect" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-26 21:10:59"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00540 ;
    skos:definition "the illusion of movement in a stationary object after prolonged viewing of a moving object" .

cogat:CAO_00690 a owl:Class ;
    rdfs:label "Overt attention" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-27 17:14:52"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00141 ;
    skos:definition "directing a sense organ at a stimulus source" .

cogat:CAO_00691 a owl:Class ;
    rdfs:label "Covert attention" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-27 17:16:42"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00141 ;
    skos:definition "the act of focusing on one of several sensory stimuli" .

cogat:CAO_00692 a owl:Class ;
    rdfs:label "Unconscious perception" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-27 17:37:41"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00707 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00669 ],
        cogat:CAO_00535 ;
    skos:definition "the process of perceiving a stimulus without being conscious of having seen it, measurable by electrophysiological response at the time and by recognition testing later" .

cogat:CAO_00693 a owl:Class ;
    rdfs:label "Autonoesis" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-07-02 19:52:38"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00277 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00169 ],
        cogat:CAO_00216 ;
    skos:altLabel "autonoetic consciousness" ;
    skos:definition "consciousness of self consistent over time, both in imagining the future and recalling the past; consciousness of subjectivity" .

cogat:CAO_00694 a owl:Class ;
    rdfs:label "Psychological refractory period" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-07-10 22:14:45"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "PRP" ;
    skos:definition "the phenomenon by which people are unable to make more than one conscious decision (and, by extension, unable to complete more than one task) within a few hundred milliseconds" .

cogat:CAO_00695 a owl:Class ;
    rdfs:label "Noesis" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-07-11 18:43:57"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00216 ;
    skos:altLabel "noetic consciousness" ;
    skos:definition "a consciousness characterized by knowing or familiarity without self-reference (contrast with autonoesis)" .

cogat:CAO_00696 a owl:Class ;
    rdfs:label "Impulsivity" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-07-18 19:26:06"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "impulsiveness" ;
    skos:definition "the tendency to act without adequately assessing context" .

cogat:CAO_00697 a owl:Class ;
    rdfs:label "Binocular rivalry" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-07-23 17:13:10"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "competition between the eyes for control of perception, particularly evident when different stimuli are presented to each eye" .

cogat:CAO_00698 a owl:Class ;
    rdfs:label "Cone of confusion" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-07-23 17:22:01"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a region around the head for which neither interaural time differences (ITDs) nor interaural loudness differences (ILDs) in a sound are not great enough for a person to localize the sound source" .

cogat:CAO_00699 a owl:Class ;
    rdfs:label "Vection" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-07-23 17:33:35"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00540 ;
    skos:definition "the illusion of movement in a stationary person caused by moving visual cues" .

cogat:CAO_00700 a owl:Class ;
    rdfs:label "Visual acuity" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-07-23 17:37:19"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00547 ],
        cogat:CAO_00001 ;
    skos:definition "a measure of the finest resolution perceivable by the eyes" .

cogat:CAO_00701 a owl:Class ;
    rdfs:label "Visual angle" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-07-23 17:43:42"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00547 ],
        cogat:CAO_00001 ;
    skos:definition "the angle a viewed object subtends at the eye, usually stated in degrees of arc, and is a function of the size of the object and its distance from the eye" .

cogat:CAO_00702 a owl:Class ;
    rdfs:label "Domain specificity" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-08-07 19:11:08"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the idea in cognitive science that the mind is divided according to the content of information being processed into specialized, possibly evolved, modules" .

cogat:CAO_00703 a owl:Class ;
    rdfs:label "Global precedence" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-08-07 21:23:11"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the idea that, all else held equal, global features are perceived earlier than are local features of a given stimulus" .

cogat:CAO_00704 a owl:Class ;
    rdfs:label "Anxiety" ;
    dc:contributor "Adam Cobb" ;
    dc:date "2013-06-14 4:19:52"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00269 ;
    skos:definition "An aversive psychophysiological state characterized by fear, worry, or concern associated with current or impending threat often elicited by general and specific interoceptive or exteroceptive cues." .

cogat:CAO_00706 a owl:Class ;
    rdfs:label "Reinstatement" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-08-09 18:36:20"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "following extinction, conditioned response returns after exposure to unconditional stimulus (US) alone" .

cogat:CAO_00709 a owl:Class ;
    rdfs:label "Transduction" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-08-13 21:40:53"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00547 ],
        cogat:CAO_00001 ;
    skos:definition "in which information from stimuli is transduced into signals to the brain" .

cogat:CAO_00710 a owl:Class ;
    rdfs:label "Phototransduction" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-08-13 21:43:57"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00547 ],
        cogat:CAO_00001 ;
    skos:definition "in which energy from visual stimuli is transduced into a signal in the retina to the brain" .

cogat:CAO_00711 a owl:Class ;
    rdfs:label "Border ownership" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-08-14 20:50:01"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00547 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00623 ],
        cogat:CAO_00621 ;
    skos:definition "the determination of a local feature (i.e. a contrast edge or line) and the object to which it belongs" .

cogat:CAO_00713 a owl:Class ;
    rdfs:label "Visual orientation" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-08-14 22:46:36"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00180 ],
        cogat:CAO_00547 ;
    skos:definition "use of visual stimulus information in perceiving orientation thereof or of the self" .

cogat:CAO_00715 a owl:Class ;
    rdfs:label "Crowding" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-08-15 21:24:45"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00544 ;
    skos:definition "target stimulus is made harder to identify when surrounded by similar non-target stimuli" .

cogat:CAO_00716 a owl:Class ;
    rdfs:label "Lexeme" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-08-15 23:43:57"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00368 ],
        cogat:CAO_00001 ;
    skos:definition "the phonological form of a given lexical item being retrieved" .

cogat:CAO_00717 a owl:Class ;
    rdfs:label "Trait anxiety" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-11-23 21:52:55"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a hyper-responsive system for threat detection, differentiated from state anxiety in duration and cause" .

cogat:CAO_00718 a owl:Class ;
    rdfs:label "Strength" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-01-07 23:04:09"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00391 ;
    skos:definition "Strength refers to a muscleand#39;s ability to generate force against physical objects." .

cogat:CAO_00719 a owl:Class ;
    rdfs:label "Balance" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-04-19 20:40:36"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00391 ;
    skos:definition "Balance allows humans to be able to orient the body in space, maintain an upright posture under static and dynamic conditions, and move without falling, by adjusting motoric control" .

cogat:CAO_00720 a owl:Class ;
    rdfs:label "Locomotion" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-01-08 0:32:46"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00391 ;
    skos:definition "Locomotion is the act of moving the body from one place to another. It requires input from the brain, spinal cord, peripheral nerves, muscular power and joint and cardiovascular health.  " .

cogat:CAO_00721 a owl:Class ;
    rdfs:label "Vestibular control" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-01-14 4:00:23"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The vestibular system transduces and processes angular and linear acceleration and deceleration of the head, enabling postural balance, locomotor control, and gaze stabilization, particularly during head movement. The vestibular system is an integral component of our sensory experience and sensory-motor function. Healthy peripheral and central vestibular anatomy is essential for functionally relevant gaze stability during head motion and postural control." .

cogat:CAO_00722 a owl:Class ;
    rdfs:label "Life satisfaction" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-01-14 8:24:15"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "One’s cognitive evaluation of life experiences; this measure is concerned with whether people like their lives or not. Life satisfaction includes both general (e.g., my life is going well) and domain-specific (e.g., I am satisfied with my family life) aspects. " .

cogat:CAO_00723 a owl:Class ;
    rdfs:label "Attentional bias" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2013-01-23 21:45:09"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "Attention bias" ;
    skos:definition "Attentional bias refers to the tendency for a particular class of stimuli to capture attention. The term is most typically used to refer to biases related to emotional content in bottom-up-capture attention tasks." .

cogat:CAO_00724 a owl:Class ;
    rdfs:label "Reception of facial communication" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-08-12 15:37:42"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The capacity to perceive someone’s emotional state non-verbally based on facial expressions. Examples include affect recognition, facial recognition and characterization." .

cogat:CAO_00725 a owl:Class ;
    rdfs:label "Productive facial communication" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-03-28 20:39:51"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The capacity to convey one’s emotional state non-verbally via facial expressions, including eye contact, expressive reciprocation, and gaze following." .

cogat:CAO_00726 a owl:Class ;
    rdfs:label "Reception of non-facial communication" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-03-28 21:04:09"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The capacity to perceive social and emotional information based on modalities other than facial expression, including non-verbal gestures, affective prosody, distress calling, cooing, etc." .

cogat:CAO_00727 a owl:Class ;
    rdfs:label "Production of non-facial communication" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-03-28 21:12:26"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The capacity to express social and emotional information based on modalities other than facial expression, including non-verbal gestures, affective prosody, distress calling, cooing, etc." .

cogat:CAO_00728 a owl:Class ;
    rdfs:label "Agency" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-03-28 21:15:37"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00418 ],
        cogat:CAO_00001 ;
    skos:definition "The ability to recognize one’s self as the agent of one’s actions and thoughts, including the recognition of one’s own body-body parts." .

cogat:CAO_00729 a owl:Class ;
    rdfs:label "Self knowledge" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-09-21 22:51:27"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The ability to make judgments about one’s current cognitive or emotional internal states, traits, and-or abilities." .

cogat:CAO_00730 a owl:Class ;
    rdfs:label "Animacy perception" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-09-18 17:08:15"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00801 ],
        cogat:CAO_00418 ;
    skos:definition "The ability to appropriately perceive that another entity is an agent (i.e., has a face, interacts contingently, and exhibits biological motion)." .

cogat:CAO_00731 a owl:Class ;
    rdfs:label "Action perception" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-04-01 17:12:44"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The ability to perceive the purpose of an action being performed by an animate entity." .

cogat:CAO_00732 a owl:Class ;
    rdfs:label "Understanding mental states" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-04-01 17:15:18"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The ability to make judgments and-or attributions about the mental state of other animate entities that allows one to predict or interpret their behaviors. Mental state refers to intentions, beliefs, desires, and emotions." .

cogat:CAO_00733 a owl:Class ;
    rdfs:label "Circadian rhythm" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-04-01 17:27:30"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "Endogenous, self-sustaining oscillations that organize the timing of biological systems to optimize physiology, behavior, and health. Circadian rhythms are synchronized by recurring environmental cues and attempt to anticipate the external environment. They also modulate homeostasis within the brain and other systems, tissues, and organs. " .

cogat:CAO_00734 a owl:Class ;
    rdfs:label "Sleep" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-04-01 17:42:37"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "Sleep and wakefulness are endogenous, recurring, behavioral states that reflect coordinated changes in the dynamic functional organization of the brain and that optimize physiology, behavior, and health. Homeostatic and circadian processes regulate the propensity for wakefulness and sleep. Sleep is a reversible state, characterized by postural recumbence, behavioral quiescence, and reduced responsiveness. It involves predictable cycling of non-rapid eye movement (NREM) and rapid eye movement (REM). It is affected by experiences during wakefulness, and has restorative and transformative effects that optimize neurobehavioral functions during wakefulness." .

cogat:CAO_00735 a owl:Class ;
    rdfs:label "Loss" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-04-01 17:46:52"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00374 ],
        cogat:CAO_00001 ;
    skos:definition "A state of deprivation of a motivationally significant con-specific, object, or situation. Loss may be social or non-social and may include permanent or sustained loss of shelter, behavioral control, status, loved ones, or relationships. The response to loss may be episodic (e.g., grief) or sustained." .

cogat:CAO_00736 a owl:Class ;
    rdfs:label "Defensive aggression" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-04-01 17:48:53"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "A response elicited by a real or perceived threat that leads to a pattern of behaviors directed at terminating the threat." .

cogat:CAO_00737 a owl:Class ;
    rdfs:label "Offensive (proactive) aggression" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-04-01 17:49:25"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "A response elicited by competition over resource acquisition or other positive consequences. This form of aggression often arises from differences in social status and dominance." .

cogat:CAO_00739 a owl:Class ;
    rdfs:label "Effort valuation" ;
    dc:contributor "Carinna Torgerson" ;
    dc:date "2013-04-01 17:52:50"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "willingness to work" ;
    skos:definition "Processes by which the cost(s) of obtaining an outcome is computed; tendency to overcome response costs to obtain a reinforcer." .

cogat:CAO_00741 a owl:Class ;
    rdfs:label "Semantic network" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2015-09-21 20:21:57"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "ontology" ;
    skos:definition "Set of concepts and relations" .

cogat:CAO_00742 a owl:Class ;
    rdfs:label "Cognitive training" ;
    dc:contributor "Faraz Farzin" ;
    dc:date "2013-05-29 23:33:46"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "Targeted exercises designed to help improve mental capacities." .

cogat:CAO_00744 a owl:Class ;
    rdfs:label "Neuroplasticity" ;
    dc:contributor "Faraz Farzin" ;
    dc:date "2013-05-29 23:36:14"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The brainand#39;s ability to change in structure and function through experience." .

cogat:CAO_00745 a owl:Class ;
    rdfs:label "Restricted behavior" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-08-10 19:02:51"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "repetitive behavior",
        "stereotyped behavior" ;
    skos:definition "A restricted behavior is a stereotyped pattern of behavior, activity, or interest. Examples include preoccupation with an object, activity, or topic, and this preoccupation is abnormal in intensity or focus.  Individuals with restricted behaviors may find it challenging to break away from habit or routine." .

cogat:CAO_00746 a owl:Class ;
    rdfs:label "Social motivation" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-08-12 15:42:09"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "An individualand#39;s tendency to seek out direct contact with another person or a group of people." .

cogat:CAO_00747 a owl:Class ;
    rdfs:label "Irritability" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-20 16:42:38"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "agitation",
        "anger",
        "grumpy",
        "moodiness",
        "short tempered",
        "testy" ;
    skos:definition "The quality or state of being agitated, testy, grumpy, moody, or having a short temper." .

cogat:CAO_00748 a owl:Class ;
    rdfs:label "Lethargy" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-20 16:45:27"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "slow",
        "socially withdrawn" ;
    skos:definition "a lack of energy or enthusiasm.  In a social context, this means being socially withdrawn." .

cogat:CAO_00749 a owl:Class ;
    rdfs:label "Hyperactivity" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-10-30 16:36:28"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "movement",
        "non-compliance",
        "restlessness" ;
    skos:definition "a condition characterized by excessive restlessness and movement, and commonly non-compliance" .

cogat:CAO_00750 a owl:Class ;
    rdfs:label "Inappropriate speech" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-20 16:49:12"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "dialogue, either to oneself or others, that is not appropriate or suitable to the current situation" .

cogat:CAO_00751 a owl:Class ;
    rdfs:label "Internalizing" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-20 19:58:37"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "anxiety",
        "depression",
        "negativity",
        "worry" ;
    skos:definition "focusing negative energy on the self, often through holding back emotion, often leading to less than ideal emotional states (worry, anxiety, negativity and depression).  These states are frequently present in autistic children." .

cogat:CAO_00752 a owl:Class ;
    rdfs:label "Externalizing" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-20 19:56:51"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "attribution bias" ;
    skos:definition "attributing cause to factors outside of the self" .

cogat:CAO_00753 a owl:Class ;
    rdfs:label "Conduct disorder" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-21 20:27:27"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "antisocial",
        "callous",
        "unemotional" ;
    skos:definition "Lacking regard for others and callous, unemotional.  Characterized by repetitive and persistent pattern of behavior in which the basic rights of others or major age-appropriate norms are violated." .

cogat:CAO_00754 a owl:Class ;
    rdfs:label "Antisocial personality" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-21 20:32:12"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "hermit",
        "introvert" ;
    skos:definition "avoiding the company of other people, unsociable, sometimes to the extent of injuring others or the interests of society in general" .

cogat:CAO_00755 a owl:Class ;
    rdfs:label "Defiance" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-21 20:36:31"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "disobedient",
        "oppositional" ;
    skos:definition "bold resistance to an opposing force, power, or authority, expressed through behavior and attitude" .

cogat:CAO_00757 a owl:Class ;
    rdfs:label "Social phobia" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-21 20:44:40"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00574 ;
    skos:altLabel "embarrassment" ;
    skos:definition "having a fear so strong of being judged by others and of being embarrassed that it significantly gets in the way of everyday life" .

cogat:CAO_00758 a owl:Class ;
    rdfs:label "Rigidity" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-21 21:06:19"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00756 ;
    skos:definition "unable to stray from a particular pattern of behavior or thought" .

cogat:CAO_00759 a owl:Class ;
    rdfs:label "Agreeableness" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-22 21:19:34"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "cooperative",
        "kind",
        "sympathetic",
        "warm" ;
    skos:definition "Agreeableness is a personality trait manifesting itself in individual behavioral characteristics that are perceived as kind, sympathetic, cooperative, warm and considerate.  http:--en.wikipedia.org-wiki-Agreeableness" .

cogat:CAO_00760 a owl:Class ;
    rdfs:label "Openness" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-22 21:22:25"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "curious",
        "imaginative",
        "open" ;
    skos:definition "Openness to experience is one of the domains which are used to describe human personality in the Five Factor Model. Openness involves active imagination, aesthetic sensitivity, attentiveness to inner feelings, preference for variety, and intellectual curiosity.  http:--en.wikipedia.org-wiki-Openness-to-experience" .

cogat:CAO_00761 a owl:Class ;
    rdfs:label "Psychosis" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-23 15:27:34"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "A kind of disordered thinking for which thought and emotions are so impaired that contact is lost with external reality." .

cogat:CAO_00762 a owl:Class ;
    rdfs:label "Paranoia" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-09-23 15:28:51"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "delusional" ;
    skos:definition "a mental condition characterized by delusions of persecution, unwarranted jealousy, or exaggerated self-importance, typically elaborated into an organized system. It may be an aspect of chronic personality disorder, of drug abuse, or of a serious condition such as schizophrenia in which the person loses touch with reality." .

cogat:CAO_00763 a owl:Class ;
    rdfs:label "Phonological awareness" ;
    dc:contributor "Julio Alberto Gonzlez Torre" ;
    dc:date "2013-09-24 2:50:04"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "Explicit awareness of the abstract units that compose spoken words, including syllables, onset and rime units, and individual phonemes." .

cogat:CAO_00764 a owl:Class ;
    rdfs:label "Guilt" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-10-11 18:04:15"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "remorse" ;
    skos:definition "a cognitive or an emotional experience that occurs when a person realizes or believes (whether it is true or not) that he or she has compromised his or her own standards of conduct or has violated a moral standard, and bears significant responsibility for that violation." .

cogat:CAO_00765 a owl:Class ;
    rdfs:label "Anhedonia" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-10-11 18:05:02"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "lack of interest" .

cogat:CAO_00766 a owl:Class ;
    rdfs:label "Appetite" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-10-11 18:06:00"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "hunger" ;
    skos:definition "the desire to eat food" .

cogat:CAO_00767 a owl:Class ;
    rdfs:label "Fatigue" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-10-11 18:07:14"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "tiredness" ;
    skos:definition "a feeling of weariness, tiredness, or lack of energy" .

cogat:CAO_00768 a owl:Class ;
    rdfs:label "Suicidal ideation" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-10-11 18:08:10"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "suicidal",
        "suicidal thoughts" ;
    skos:definition "thoughts about or an unusual preoccupation with suicide" .

cogat:CAO_00769 a owl:Class ;
    rdfs:label "Perfectionism" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-10-14 20:50:01"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a personality trait characterized by a personand#39;s striving for flawlessness and setting excessively high performance standards, accompanied by overly critical self-evaluations and concerns regarding othersand#39; evaluations." .

cogat:CAO_00770 a owl:Class ;
    rdfs:label "Eating" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-10-15 18:38:01"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "appetite",
        "food preference" ;
    skos:definition "An individualand#39;s preferences for food or general appetite." .

cogat:CAO_00771 a owl:Class ;
    rdfs:label "Sensory defensiveness" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-10-15 18:43:29"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "sensitivity" ;
    skos:definition "a condition defined as having and#34;a tendency to react negatively or with alarm to sensory input which is generally considered harmless or non-irritatingand#34; to neurotypical people.and#34;" .

cogat:CAO_00772 a owl:Class ;
    rdfs:label "Sensitivity to change" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-10-15 18:48:57"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "flexibility",
        "tolerance" ;
    skos:definition "an individualand#39;s ability to withstand changes in his or her environment or situation." .

cogat:CAO_00773 a owl:Class ;
    rdfs:label "Noise sensitivity" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-10-15 18:53:50"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "auditory sensitivity" ;
    skos:definition "having a tendency to react negatively or with alarm to noise input which is generally considered harmless or non-irritating to neurotypical individuals." .

cogat:CAO_00774 a owl:Class ;
    rdfs:label "Loneliness" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2013-12-02 22:44:50"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "alone",
        "isolation",
        "solitary" ;
    skos:definition "loneliness is a complex and unpleasant emotional response to isolation or lack of companionship.  http:--en.wikipedia.org-wiki-Loneliness" .

cogat:CAO_00776 a owl:Class ;
    rdfs:label "Conflict adaptation effect" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-09-18 16:54:21"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "conflict adaptation",
        "trial-to-trial conflict effect" ;
    skos:definition "The congruency effect after incongruent trials is reduced when compared with congruent trials. It is a trial-to-trial effect. Originally reported by Gratton, Coles, and Donchin (1992; as refered by van Steenbergen, 2010; Psychological Science). Thus: (iI-iC) " .

cogat:CAO_00778 a owl:Class ;
    rdfs:label "Goal selection" ;
    dc:contributor "Corey White" ;
    dc:date "2015-03-30 20:45:37"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00636 .

cogat:CAO_00781 a owl:Class ;
    rdfs:label "Visual localization" ;
    dc:contributor "Corey White" ;
    dc:date "2015-04-13 22:30:24"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00547 ],
        cogat:CAO_00788 ;
    skos:definition "Localization of visually perceived objects relative to other visually perceived objects (either simultaneously or successively) or relative to a visual norm, or by absolute identification. " .

cogat:CAO_00782 a owl:Class ;
    rdfs:label "Perceptual priming" ;
    dc:contributor "Corey White" ;
    dc:date "2015-04-05 20:59:50"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00160 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00547 ],
        cogat:CAO_00669 ;
    skos:definition "is based on the form of the stimulus and is enhanced by the match between the early and later stimuli." .

cogat:CAO_00783 a owl:Class ;
    rdfs:label "Spatial localization" ;
    dc:contributor "Corey White" ;
    dc:date "2015-04-05 20:55:28"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00160 ],
        cogat:CAO_00788 ;
    skos:definition "it is the reference of a visual sensation to a definite locality in space" .

cogat:CAO_00784 a owl:Class ;
    rdfs:label "Limited capacity" ;
    dc:contributor "Corey White" ;
    dc:date "2015-04-03 22:09:06"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00559 ],
        cogat:CAO_00001 .

cogat:CAO_00785 a owl:Class ;
    rdfs:label "Interference control" ;
    dc:contributor "Corey White" ;
    dc:date "2015-04-03 22:18:35"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00559 ],
        cogat:CAO_00001 .

cogat:CAO_00786 a owl:Class ;
    rdfs:label "Exogenous attention" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2015-09-21 20:47:15"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00820 ],
        cogat:CAO_00141 ;
    skos:altLabel "stimulus-driven attention" ;
    skos:definition "ADD DEFINITION HERE" .

cogat:CAO_00787 a owl:Class ;
