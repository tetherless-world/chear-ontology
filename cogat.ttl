cogat:CAO_00114 a owl:Class ;
    rdfs:label "Affect recognition" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 15:46:31"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The ability to understand a physical expression that serves as an indicator of emotion." .

cogat:CAO_00116 a owl:Class ;
    rdfs:label "Altruistic motivation" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 15:46:51"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "A desire or need to help others driven by selflessness." .

cogat:CAO_00117 a owl:Class ;
