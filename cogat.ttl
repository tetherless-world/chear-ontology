cogat:CAO_00489 a owl:Class ;
    rdfs:label "Sequence learning" ;
    dc:contributor "Akram Bakkour" ;
    dc:date "2011-08-17 17:06:58"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00364 ;
    skos:definition "learning of a sequence of items or responses in the precise order of their presentation." .

cogat:CAO_00495 a owl:Class ;
    rdfs:label "Short term memory" ;
    dc:contributor "Akram Bakkour" ;
    dc:date "2011-08-19 16:46:00"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00380 ;
    skos:definition "A limited-capacity and short-lasting representation of information in the mind. The duration of short-term memory is on the order of seconds, while its capacity is on the order of 4 to 9 independent items." .

cogat:CAO_00497 a owl:Class ;
    rdfs:label "Skill" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-09-18 16:52:12"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition " Proficiency, facility, or dexterity that is acquired or developed through training or experience." .

cogat:CAO_00500 a owl:Class ;
    rdfs:label "Social cognition" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 1:38:21"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "the encoding, storage, retrieval, and processing, in the brain, of information relating to conspecifics, or members of the same species." .

cogat:CAO_00507 a owl:Class ;
    rdfs:label "Spatial attention" ;
    dc:contributor "Agatha Lenartowicz" ;
    dc:date "2009-07-10 21:06:32"^^<xsd:dateTime> ;
