    dc:date "2015-11-30 4:50:41"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00418 ;
    skos:altLabel "gustation" ;
    skos:definition "Form of chemoreception that facilitates taste perception." .

cogat:CAO_00632 a owl:Class ;
    rdfs:label "Rehearsal" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 1:16:47"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00495 ],
        cogat:CAO_00001 ;
    skos:definition "The repetition of information in an attempt to maintain it longer in memory." .

cogat:CAO_00640 a owl:Class ;
    rdfs:label "Joint attention" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-29 0:11:43"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "mutual knowledge" ;
    skos:definition "the process of alerting one person to a stimulus through nonverbal cues such as finger pointing or gazing. It is one of the first signs of the development of a theory of mind in babies and serves as an important step to later language and social development." .

cogat:CAO_00644 a owl:Class ;
    rdfs:label "Interference resolution" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2013-01-23 21:47:34"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00559 ],
        cogat:CAO_00636 ;
    skos:altLabel "interference control" ;
