    dc:contributor "Russ Poldrack" ;
    dc:date "2015-05-20 21:24:25"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00812 ;
    skos:altLabel "relational matching" ;
    skos:definition "Comparison of two stimuli based on a relational feature" .

cogat:CAO_00801 a owl:Class ;
    rdfs:label "Animacy decision" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2015-12-30 17:06:59"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00246 ;
    skos:definition "decision about whether a stimulus exhibits animacy" .

cogat:CAO_00803 a owl:Class ;
    rdfs:label "Auditory tone discrimination" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-09-08 21:27:42"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00160 ;
    skos:altLabel "sound discrimination",
        "tone discrimination" ;
    skos:definition "It is the ability to perceive and respond to differences among auditory stimuli" .

cogat:CAO_00809 a owl:Class ;
    rdfs:label "Emotional reappraisal" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-06-12 20:59:48"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00743 ;
    skos:definition "ADD DEFINITION HERE" .

cogat:CAO_00813 a owl:Class ;
    rdfs:label "Loss anticipation" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-09-08 22:21:59"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00127 ;
    skos:definition "prior expectation of the occurrence of a loss, sometimes accompanied by an emotional reaction" .

