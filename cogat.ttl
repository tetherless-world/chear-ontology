    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-26 18:09:40"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "JND",
        "just noticeable difference" ;
    skos:definition "the smallest detectable difference between two stimuli" .

cogat:CAO_00687 a owl:Class ;
    rdfs:label "Attentional blink" ;
    dc:contributor "Mark Law" ;
    dc:date "2012-12-15 23:29:08"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:altLabel "AB" ;
    skos:definition "a phenomenon in which the participant is unable to detect a second salient visual stimulus if presented at the same spatial location within 200-500 milliseconds after the first" .

cogat:CAO_00708 a owl:Class ;
    rdfs:label "Feature integration" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-08-13 19:24:22"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "how different features of a percept are bound into a conscious whole" .

cogat:CAO_00712 a owl:Class ;
    rdfs:label "Multistable perception" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-08-14 21:10:03"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00160 ;
    skos:definition "conflicting sensory input results in spontaneous, sometimes controllable, experiences thereof" .

cogat:CAO_00714 a owl:Class ;
