    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-21 21:19:23"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00123 ;
    skos:definition "the structural comparison of a familiar with a novel situation" .

