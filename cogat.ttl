cogat:CAO_00124 a owl:Class ;
    rdfs:label "Analogical transfer" ;
    dc:contributor "Russ Poldrack" ;
    dc:date "2012-03-28 15:48:50"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The transfer of knowledge from one situation to another by finding a set of one-to-one correspondences between aspects of one body of information and aspects of another." .

