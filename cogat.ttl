    skos:definition "The cognitive process of producing words on ones own term instead of extracting them from an outside source." .

cogat:CAO_00557 a owl:Class ;
    rdfs:label "Word recognition" ;
    dc:contributor "Marc Brysbaert" ;
    dc:date "2015-08-28 6:56:01"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00458 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00854 ],
        cogat:CAO_00616 ;
    skos:definition "the ability of a reader to recognize words correctly and virtually effortlessly." .

cogat:CAO_00567 a owl:Class ;
    rdfs:label "Decision" ;
    dc:contributor "Tom Schonberg" ;
    dc:date "2009-07-10 21:06:33"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "The outcome of a process during which a choice is made, usually between several possible options" .

cogat:CAO_00573 a owl:Class ;
