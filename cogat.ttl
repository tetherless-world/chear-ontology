        [ a owl:Restriction ;
            owl:onProperty sio:hasPart ;
            owl:someValuesFrom cogat:CAO_02239 ],
        cogpo:COGPO_00049 ;
    skos:definition "Subjects are presented with a 3 digit number and are asked to respond which of the digits differ from the other two." .

