cogat:CAO_01581 a owl:Class ;
    rdfs:label "Penn motor praxis" ;
    dc:contributor "Alfonso Bonilla" ;
    dc:date "2016-01-22 22:23:59"^^<xsd:dateTime> ;
    rdfs:subClassOf cogpo:COGPO_00049 ;
    skos:altLabel "Mouse Practice",
