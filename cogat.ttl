    dc:date "2009-07-10 21:06:32"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00459 ;
    skos:definition "Broadly, the mental processes involved in finding a solution to a problem." .

cogat:CAO_00451 a owl:Class ;
    rdfs:label "Proprioception" ;
    dc:contributor "Brenda Gregory" ;
    dc:date "2011-05-17 17:02:47"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00503 ;
    skos:definition "the sense of the relative position of neighboring parts of the body." .

cogat:CAO_00467 a owl:Class ;
    rdfs:label "Response selection" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-11-08 23:32:53"^^<xsd:dateTime> ;
