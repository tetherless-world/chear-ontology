            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00559 ],
        cogat:CAO_00636 ;
    skos:definition "The revision or refreshing of information that is maintained in working memory" .

cogat:CAO_00665 a owl:Class ;
    rdfs:label "Sense of ownership" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-26 18:03:28"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00654 ;
    skos:definition "The feeling that something is part of one self." .

cogat:CAO_00672 a owl:Class ;
    rdfs:label "Embodied cognition" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-05-16 20:42:45"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "a theory emphasizing the essentiality of the bodyand#39;s interaction with the environment to cognition" .

cogat:CAO_00673 a owl:Class ;
    rdfs:label "Resource" ;
    dc:contributor "Michael Sharman" ;
    dc:date "2012-02-14 14:41:36"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00001 ;
    skos:definition "any aspect of cognition having bounded availability, eg. memory, attention" .

cogat:CAO_00675 a owl:Class ;
    rdfs:label "Visual awareness" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-07-03 22:08:57"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00418 ;
    skos:definition "the subjective experience of seeing, emerging about 200ms after the onset of a visual stimulus; it is contrasted with unconscious visual perception" .

cogat:CAO_00680 a owl:Class ;
    rdfs:label "Inattentional blindness" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-21 20:38:22"^^<xsd:dateTime> ;
    rdfs:subClassOf cogat:CAO_00328 ;
    skos:altLabel "perceptual blindness" ;
    skos:definition "a phenomenon in which people fail to see an unexpected stimulus" .

cogat:CAO_00682 a owl:Class ;
    rdfs:label "Extinction" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-06-25 23:01:26"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00705 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00417 ],
        [ a owl:Restriction ;
            owl:onProperty sio:isPartOf ;
            owl:someValuesFrom cogat:CAO_00339 ],
        cogat:CAO_00221 ;
    skos:definition "the phenomenon by which a response to a conditional stimulus presented in absence of the unconditional stimulus diminishes over time; rather than a forgetting, it is generally believed to be a new learning." .

cogat:CAO_00683 a owl:Class ;
